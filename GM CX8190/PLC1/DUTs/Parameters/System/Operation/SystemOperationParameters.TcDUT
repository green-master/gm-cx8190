	SystemName: STRING := 'GM'; // System name |||| Systemnamn
	StartupHeatRecoveryTime: LTIME := LTIME#3M;
	SupplyFanStartupDelay: LTIME := LTIME#1M;
	PostVentilationTime: LTIME := LTIME#2M;
	PostFireVentilationTime: LTIME := LTIME#0M;
