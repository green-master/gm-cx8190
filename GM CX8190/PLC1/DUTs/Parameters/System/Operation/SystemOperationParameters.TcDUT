													TC03 := ( Day := TimerDays.Wednesday, OnTime := 0530, OffTime := 2000 ),
													TC04 := ( Day := TimerDays.Thursday, OnTime := 0530, OffTime := 2000 ),
													TC05 := ( Day := TimerDays.Friday, OnTime := 0530, OffTime := 2000 ),
													TC06 := ( Day := TimerDays.Saturday, OnTime := 0530, OffTime := 2000 ),
													TC07 := ( Day := TimerDays.Sunday, OnTime := 0530, OffTime := 2000 )
												);
END_STRUCT
END_TYPE
